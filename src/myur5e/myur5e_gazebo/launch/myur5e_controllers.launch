<?xml version="1.0"?>
<launch>
  
  <!-- Controllers config -->
  <rosparam file="$(find myur5e_gazebo)/controller/joint_state_controller.yaml" command="load"/>
  <rosparam file="$(find myur5e_gazebo)/controller/myur5e_arm_controller.yaml" command="load"/>
  <rosparam file="$(find myur5e_gazebo)/controller/my2f85_gripper_controller.yaml" command="load"/>
  <!-- PID config -->
  <!-- <rosparam file="$(find myur5e_gazebo)/controller/pid_gain_gazebo.yaml" command="load"/> -->

  <!-- Load controllers -->
  <node name="robot_controllers" pkg="controller_manager" type="controller_manager" respawn="false" output="screen" 
      args="spawn joint_state_controller myur5e_arm_controller my2f85_gripper_controller" />

  <!-- Robot state publisher -->
  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
    <param name="publish_frequency" type="double" value="50.0" />
    <param name="tf_prefix" type="string" value="" />
  </node>

  <!-- Fake Calibration -->
  <node pkg="rostopic" type="rostopic" name="fake_joint_calibration"
        args="pub /calibrated std_msgs/Bool true" />

</launch>
